<?xml version="1.0" encoding="UTF-8"?>
<endpoint-groups xmlns="urn:ietf:params:xml:ns:yang:ietf-i2nsf-cons-facing-interface">
  <!-- No specific endpoint groups defined in the policy -->
</endpoint-groups>
<i2nsf-cfi-policy xmlns="urn:ietf:params:xml:ns:yang:ietf-i2nsf-cons-facing-interface">
  <name>Restrict_Adult_Content_Access</name>
  <rules>
    <name>Restrict_Access_From_All_Devices</name>
    <condition>
      <firewall>
        <source>all-devices-placeholder</source>
      </firewall>
      <url-category>
        <url-name>adult-content-placeholder</url-name>
      </url-category>
    </condition>
    <action>
      <primary-action>
        <action>drop</action>
      </primary-action>
    </action>
  </rules>
  <language>en-US</language>
  <priority-usage>priority-by-order</priority-usage>
  <resolution-strategy>fmr</resolution-strategy>
</i2nsf-cfi-policy>

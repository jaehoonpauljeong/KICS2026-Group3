<?xml version="1.0" encoding="UTF-8" ?>
<i2nsf-cfi-policy
    xmlns="urn:ietf:params:xml:ns:yang:ietf-i2nsf-cons-facing-interface">
    <name>discard_unknown_traffic_policy</name>
    <rules>
        <name>discard_unknown_traffic</name>
        <condition>
            <firewall-condition>
                <source>unknown</source>
                <destination>unclassified</destination>
            </firewall-condition>
        </condition>
        <actions>
            <primary-action>
                <action>drop</action>
            </primary-action>
        </actions>
    </rules>
</i2nsf-cfi-policy>
